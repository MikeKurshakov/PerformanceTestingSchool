              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">int usersAmount = Integer.parseInt( vars.get( &apos;EDITOR_ROLES_matchNr&apos; ) );
switch ( usersAmount )
{
	case { usersAmount &lt; 10 }:
	vars.put( &apos;ACTION&apos; , &apos;Add User&apos; );
	break;
	case { usersAmount &gt;= 10 &amp;&amp; usersAmount &lt;= 20}:
	if( new Random().nextInt(2).toString().toBoolean() )
	vars.put( &apos;ACTION&apos; , &apos;Add User&apos; );
	else
	vars.put( &apos;ACTION&apos; , &apos;Delete user&apos; );
	break;
	case { usersAmount &gt; 20 }:
	vars.put( &apos;ACTION&apos; , &apos;Delete user&apos; );
	break;
}</stringProp>
