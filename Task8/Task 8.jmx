              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">int count = Integer.parseInt( vars.get( &apos;LAST_USER_matchNr&apos; ) );
if( count != 0 ){
	ArrayList strList = new ArrayList();
	for (int i=0; i&lt;count; i++ ){
		strList.add( Integer.parseInt( vars.get( &apos;LAST_USER_&apos; + String.valueOf(i+1) ) ) );
		}
	Collections.sort( strList );
	vars.putObject( &apos;LAST_USER&apos; , strList.last() );
	vars.putObject( &apos;user_counter&apos; , strList.last() );
	}
else {
	vars.putObject( &apos;LAST_USER&apos; , &apos;1&apos; );
	vars.putObject( &apos;user_counter&apos; , &apos;0&apos; );
	}
int res = Integer.parseInt( vars.get( &apos;user_counter&apos; ) );
vars.putObject( &apos;user_counter&apos; , res+1 );
	</stringProp>
